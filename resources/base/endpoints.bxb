endpoints {
  authorization {
    none
  }
  action-endpoints {
    action-endpoint (FindRoutine) {
      accepted-inputs (name, type, maxCnt)
      local-endpoint ("FindRoutine.js")
    }

    action-endpoint (Counting) {
      accepted-inputs (__LIST_OF_ACTION_INPUTS__)
      remote-endpoint (__ENDPOINT_URL__) {
        method (__HTTP_METHOD_USED__)
      }
    }
  }
}
